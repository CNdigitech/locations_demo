<div class="my-form col-lg-6 ">
  
  <%= simple_form_for(@election_event_location) do |f| %>
    <%= f.error_notification %>
    <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

    <div class="form-inputs">
      <%= f.input :assigned_code %>
      <%= f.input :election_event_id %>
      <%= f.input :region_id %>
      <%= f.input :constituency_id %>
    </div>

    <div class="form-actions">
      <%= f.button :submit, class:"btn btn-md btn-primary" %>
      <%= link_to 'Back', election_event_locations_path, class:"btn btn-md btn-outline-dark"%>
    </div>
  <% end %>
</div>