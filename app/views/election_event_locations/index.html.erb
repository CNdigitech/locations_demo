
   
    <div class="my-content"> 
        <h2> election_event_locations</h2>
        <div class="new-btn float-right">
            <%= link_to new_election_event_location_path, class:"btn btn-outline-dark" do %>
                <span class="glyphicon glyphicon-plus"></span>
                New election_event_location
            <% end %>
        </div>

        <div class="table-responsive">
            <table class="table table-striped table-hover">
                <thead class="thead-dark">
                <tr>
                <th>#</th>
                                <th>Assigned code</th>
                                <th>Election event</th>
                                <th>Region</th>
                                <th>Constituency</th>
                                <th colspan="2"></th>
                </tr>
                </thead>

                <tbody>
                <% @election_event_locations.each_with_index do |election_event_location, index| %>
                    <tr>
                        <td><%= index + 1 %></td>
                                            <td><%= election_event_location.assigned_code %></td>
                                            <td><%= election_event_location.election_event_id %></td>
                                            <td><%= election_event_location.region_id %></td>
                                            <td><%= election_event_location.constituency_id %></td>
                                            
                        <td><%= link_to 'Edit', edit_election_event_location_path(election_event_location), class:"btn btn-sm btn-outline-dark" %></td>
                        <td><%= link_to 'Delete', election_event_location, method: :delete, data: { confirm: 'Are you sure?' }, class:"btn btn-sm btn-danger" %></td>
                    </tr>
                <% end %>
                </tbody>
            </table>
        </div>
    </div>
