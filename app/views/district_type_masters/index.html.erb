
   
    <div class="my-content"> 
        <h2> District Types</h2>
        <div class="new-btn float-right">
            <%= link_to new_district_type_master_path, class:"btn btn-outline-dark" do %>
                <span class="glyphicon glyphicon-plus"></span>
                New District Type
            <% end %>
        </div>

        <div class="table-responsive">
            <table class="table table-striped table-hover">
                <thead class="thead-dark">
                <tr>
                    <th>#</th>
                    <th>Assigned code</th>
                    <th>Name</th>
                    <th></th>
                    <th></th>
                    <th></th>
                </tr>
                </thead>

                <tbody>
                    <% @district_type_masters.each_with_index do |district_type_master,index| %>
                        <tr>
                            <td><%= index+1 %>
                            <td><%= district_type_master.assigned_code %></td>
                            <td><%= district_type_master.name %></td>
                            <td><%= link_to 'Show', district_type_master, class:"btn btn-sm btn-default" %></td>
                            <td><%= link_to 'Edit', edit_district_type_master_path(district_type_master), class:"btn btn-sm btn-default" %></td>
                            <td><%= link_to 'Destroy', district_type_master, method: :delete, data: { confirm: 'Are you sure?' }, class:"btn btn-sm btn-default" %></td>
                        </tr>
                    <% end %>
                </tbody>
            </table>
        </div>


