<div class="my-form col-lg-6 col-lg-offset-3">
  
  <%= simple_form_for(@district_type_master) do |f| %>
    <%= f.error_notification %>
    <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

    <div class="form-inputs">
      <%= f.input :assigned_code %>
      <%= f.input :name %>
    </div>

    <div class="form-actions">
      <%= f.button :submit, class:"btn btn-md btn-primary" %>
      <%= link_to 'Back', district_type_masters_path, class:"btn btn-md btn-outline-dark" %>
    </div>
  <% end %>
</div>