
   
    <div class="my-content"> 
        <h2> Election Events </h2>
        <div class="new-btn float-right">
            <%= link_to new_election_event_master_path, class:"btn btn-outline-dark" do %>
                <span class="glyphicon glyphicon-plus"></span>
                New Election Event
            <% end %>
        </div>

        <div class="table-responsive">
            <table class="table table-striped table-hover">
                <thead class="thead-dark">
                <tr>
                <th>#</th>
                                <th>Election event</th>
                                <th>Event name</th>
                                <th>Election type</th>
                                <th>Election date</th>
                                <th>Notes</th>
                                <th>Ec election reference</th>
                                <th>Active status</th>
                                <th>Del status</th>
                                <th>User</th>
                                <th colspan="2"></th>
                </tr>
                </thead>

                <tbody>
                <% @election_event_masters.each_with_index do |election_event_master, index| %>
                    <tr>
                        <td><%= index + 1 %></td>
                                            <td><%= election_event_master.election_event_id %></td>
                                            <td><%= election_event_master.event_name %></td>
                                            <td><%= election_event_master.election_type %></td>
                                            <td><%= election_event_master.election_date %></td>
                                            <td><%= election_event_master.notes %></td>
                                            <td><%= election_event_master.ec_election_reference %></td>
                                            <td><%= election_event_master.active_status %></td>
                                            <td><%= election_event_master.del_status %></td>
                                            <td><%= election_event_master.user_id %></td>
                                            
                        <td><%= link_to 'Edit', edit_election_event_master_path(election_event_master), class:"btn btn-sm btn-outline-dark" %></td>
                        <td><%= link_to 'Delete', election_event_master, method: :delete, data: { confirm: 'Are you sure?' }, class:"btn btn-sm btn-danger" %></td>
                    </tr>
                <% end %>
                </tbody>
            </table>
        </div>
    </div>
